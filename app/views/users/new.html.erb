<h1>Create Account</h1>
    <%= form_for(@user) do |f| %>
    <ul>
    <% @user.errors.full_messages.each do |error|%>
        <li><%= error %></li>
    <% end %>
    </ul>

    <p>
        <%= f.label :username %></br />
        <%= f.text_field :username %>
    </p>

    <p>
        <%= f.submit "Create Account" %>
    </p>
<% end %>

    